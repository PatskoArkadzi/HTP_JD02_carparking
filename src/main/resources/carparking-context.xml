<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
           
	<bean name="view_car_list"
		class="by.htp.carparking.web.action.impl.CarListViewAction">
		<property name="carService" ref="carService"/>
	</bean>
	<bean name="order_car"
		class="by.htp.carparking.web.action.impl.OrderCarAction">
		<property name="orderService" ref="orderService" />
		<property name="carService" ref="carService"/>
	</bean>
	<bean name="car_create"
		class="by.htp.carparking.web.action.impl.CarCreateAction">
		<property name="carService" ref="carService"/>
	</bean>
	<bean name="car_update"
		class="by.htp.carparking.web.action.impl.CarUpdateAction">
		<property name="carService" ref="carService"/>
	</bean>
	<bean name="car_read"
		class="by.htp.carparking.web.action.impl.CarReadAction">
		<property name="carService" ref="carService"/>
	</bean>
	
	
	
	<bean name="carService"
		class="by.htp.carparking.service.impl.CarServiceImpl">
		<property name="carDao" ref="carDao"></property>
	</bean>
	<bean name="orderService"
		class="by.htp.carparking.service.impl.OrderServiceImpl">
		<property name="orderDao" ref="orderDao"></property>
	</bean>
	
	<bean name="carDao" class="by.htp.carparking.db.dao.impl.CarDaoDataBaseImpl">
	</bean>
	<bean name="orderDao" class="by.htp.carparking.db.dao.impl.OrderDaoDataBaseImpl">
	</bean>


